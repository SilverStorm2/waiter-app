{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "framework": "create-react-app",
  "installCommand": "yarn install --frozen-lockfile",
  "buildCommand": "yarn build",
  "functions": {
    "api/**/*.js": {
      "includeFiles": "db.json"
    }
  }
}
